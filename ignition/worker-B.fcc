variant: fcos
version: 1.3.0
passwd:
  users:
    - name: core
      ssh_authorized_keys:
        - ssh-rsa  faisalsalhi@MacBook-Pro-de-Faisal-2.local
    - name: k8s_cp
      groups:
        - sudo
        - wheel
        - docker
      ssh_authorized_keys:
        - ssh-rsa  faisalsalhi@MacBook-Pro-de-Faisal-2.local
storage:
  files:
    - path: "/var/lib/kubelet/worker-B-key.pem"
      contents:
        source: "s3://saasproj/instances/worker-B-key.pem"
    - path: "/var/lib/kubelet/worker-B.pem"
      contents:
        source: "s3://saasproj/instances/worker-B.pem"
    - path: "/var/lib/kubelet/kubeconfig/worker-B.kubeconfig"
      contents:
        source: "s3://saasproj/instances/worker-B.kubeconfig"
    - path: "/var/lib/kubernetes/ca.pem"
      contents:
        source: "s3://saasproj/instances/ca.pem"
    - path: "/var/lib/kube-proxy/kubeconfig/kube-proxy.kubeconfig"
      contents:
        source: "s3://saasproj/instances/kube-proxy.kubeconfig"
